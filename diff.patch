+(defn not-found-page []
+  [:section.section>div.container>div.content
+   [:p "404 (страница не найдена)"]
+   [:p  [:a {:href (href :search)} "назад"]]])
+
+(defn page []
+  (let [current-route @(subscribe [:current-route])]
