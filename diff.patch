+												{% else %}
+													<em>{{ 'message_form_group_empty'|trans({}, 'SonataAdminBundle') }}</em>
+												{% endfor %}
+											</div>
+										</div>
+									</div>
+								</div>
+							{% endfor %}
+						</div>
+					</div>
+				{% endblock %}
+
+				{% block sonata_post_fieldsets %}
+				</div>
+			{% endblock %}
+
+			{{ form_rest(form) }}
+
+			{% block formactions %}
+				<div class="sonata-ba-form-actions well well-small form-actions">
